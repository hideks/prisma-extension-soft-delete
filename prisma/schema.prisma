generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int @id @default(autoincrement())
  name      String
  email     String @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime?
  deletedAt DateTime?
}
